# Ignore version control
.git
.gitignore
.gitattributes

# Ignore IDE/editor configs
.vscode/
.idea/
*.swp
*.swo

# Ignore Python cache and test cache
__pycache__/
*.py[cod]
*$py.class
.pytest_cache/

# Ignore virtual environments
venv/
env/
.venv/
.env

# Ignore logs and temp files
*.log
logs/
tmp/
temp/
.tmp/
*.tmp
*.bak
*.old

# Ignore test files and test data
tests/
*_test.py
test_*.py

# Ignore development dependencies
requirements-dev.txt

# Ignore MLflow runs and local DB (but NOT requirements or config)
mlruns/
mlflow.db

# Ignore local development data (but NOT data/curated or data/collected)
data/raw/
notebooks/development/

# Ignore coverage reports
htmlcov/
.coverage
.coverage.*

# Ignore security sensitive files
.env
.env.local
.env.production
secrets/

# Ignore Jupyter notebooks for development
*.ipynb

# Ignore sample/demo files
examples/
samples/
demo/